{
  "name": "memstat",
  "version": "0.1.0",
  "description": "memory usage plotter for integration tests",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "npx mocha"
  },
  "mocha": {
    "exit": true,
    "check-leaks": true,
    "colors": true,
    "timeout": 15000,
    "slow": 10000,
    "spec": [
      "test/*.spec.js"
    ]
  },
  "keywords": [
    "memory",
    "memory-leak",
    "mocha",
    "unit-test",
    "nodejs"
  ],
  "author": "@nicholaswmin",
  "license": "MIT",
  "dependencies": {
    "asciichart": "^1.5.25",
    "single-line-log": "^1.1.2",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "mocha": "^10.4.0"
  }
}
